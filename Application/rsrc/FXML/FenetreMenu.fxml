<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>


<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.FenetreMenu">
   <center>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" text="Thermostat" onAction="#clicBoutonSlider" fx:id="boutonSlider">
               <HBox.margin>
                  <Insets right="20.0"/>
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" text="Image" onAction="#clicBoutonImage"/>
         </children>
      </HBox>
   </center>
</BorderPane>
